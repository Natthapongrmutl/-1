 #include <stdio.h>
 int *GetSet(int *n){

    int arr[100];

    printf("enter number: ");
    scanf("%d", n);

    for (int i = 0; i < *n; i++)
    {
        printf("enter element %d: ");
        scanf("%d", &arr[i]);
    }

    return arr;

 }

 int main(){
    
    int *data, num;
    data = GetSet(&num);

    for (int i = 0; i < num; i++)
    {
        printf("%d", data[i]);
    }

    return 0;

 }
