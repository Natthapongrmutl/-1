#include <stdio.h>

void GetSet( int data[], int *number );

int main(){
    int data[100];
    int number;

    GetSet(data, &number);

    return 0;
}

void GetSet(int data[],int *number){
    printf("Enter number of elements: ");
        if(scanf("%d", number) != 1){
            printf("Not number ");
            return ;
    }

    for(int i = 0; i < *number; i++){
        printf("Enter Number %d: ", i+1);
        if(scanf("%d",&data[i]) != 1){
            printf("Not number ");
            data[i] = 0;
        }
    }

    printf("\nYour number is:\n");
    for(int i = 0; i < *number; i++){
        printf("%d",data[i]);
    }
}
